    "fs": "0.0.1-security",
    "html-webpack-plugin": "^5.5.0",
    "line-api": "^0.2.1",
    "lodash": "^4.17.21",
    "mqtt": "^4.3.6",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "node-rtsp-stream-jsmpeg": "^0.0.2",
    "passport": "^0.5.2",
